    <profile id="f884-552c-9917-3f45" name="Implacable Officer" book="Imperial Armour - Index - Forces of the Astra Militarum" page="57" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
