      </characteristics>
    </profile>
    <profile id="3f80-6854-b4b4-b594" name="Refractor Field" book="Imperial Armour - Index - Forces of the Astra Militarum" page="57" hidden="false" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
